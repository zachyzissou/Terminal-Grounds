{
  "editor.formatOnSave": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.eol": "\n",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "github.copilot.chat.codeGeneration.useInstructionFiles": true,
  "codewrx-unreal-agent.promptVersionStamp": "1.0.0",
  "codewrx-workspace.instructionsCleared": false,
  "codewrx-workspace.setupCompleted": true,

  "cmake.buildDirectory": "${workspaceFolder}/.vscode/build",
  "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
  "C_Cpp.intelliSenseEngineFallback": "Enabled",
  "files.associations": {
    "*.Build.cs": "csharp",
    "*.Target.cs": "csharp"
  },
  "ue.engineRoot": "C:/Program Files/Epic Games/UE_5.6",
  "editor.experimentalGpuAcceleration": "on",
  "terminal.integrated.enableImages": true
}
